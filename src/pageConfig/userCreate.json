{
  "formSchema": {
    "type": "object",
    "properties": {
      "username": {
        "ui": {
          "label": "用户名称",
          "widget": "nf-input"
        },
        "rules": {
          "required": {
            "value": true,
            "errMsg": "请填写用户名称"
          }
        }
      },
      "password": {
        "type": "string",
        "ui": {
          "label": "用户密码",
          "widget": "nf-input"
        },
        "rules": {
          "required": {
            "value": true,
            "errMsg": "请填写用户密码"
          }
        }
      },
      "mobile": {
        "ui": {
          "label": "用户电话",
          "widget": "nf-input"
        }
      },
      "email": {
        "ui": {
          "label": "用户邮箱",
          "widget": "nf-input"
        }
      },
      "roleId": {
        "type": "string",
        "ui": {
          "label": "用户角色",
          "widget": "nf-select",
          "widgetConfig": {
            "enumSourceRemote": {
              "remoteUrl": "/api/enum/findRoles",
              "paramName": "keyword",
              "otherParams": {},
              "resField": "list",
              "selectFirstItem": false
            }
          }
        },
        "rules": {
          "required": {
            "value": true,
            "errMsg": "请选择用户角色"
          }
        }
      }
    },
    "ui": {
      "widgetConfig": {
        "layout": "h"
      }
    },
    "globalConfig": {
      "style": {
        "formCls": "nf-form",
        "invalidFeedbackCls": [
          "el-form-item__error"
        ]
      }
    }
  }
}
